  "bugs": {
    "url": "https://github.com/aidenybai/hacky/issues"
  },
  "homepage": "https://github.com/aidenybai/hacky#readme",
  "dependencies": {
    "htm": "^3.1.0",
    "million": "^1.3.2"
  },
  "devDependencies": {
    "rollup": "^2.66.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"

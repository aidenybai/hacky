{
  "name": "hacky",
  "version": "0.3.6",
  "description": "<2kb tagged template alternative for Crank.js",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "jsdelivr": "dist/index.mjs",
  "unpkg": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsup"
  },
  "files": [
    "dist"
